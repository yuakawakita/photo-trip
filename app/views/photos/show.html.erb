<h1></h1>
<h3>詳細</h3>
<div class="photo">
<% @photo.tags.each do |tag| %>
  <span><%= tag.name %></span>
<% end %>
  <p><%= @photo.name %></p>
  <p><%= @photo.address %></p>
  <p><%= @photo.season %></p>
  <p><%= @photo.image %></p>
  <p><%= @photo.created_at %></p>
</div>

<%= link_to "編集する", edit_photo_path(@photo.id) %>
<%= link_to "一覧に戻る", photos_path %>